{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Note",
    "description": "A note in notes",
    "type": "object",
    "properties": {
        "noteId": {
            "description": "The unique identifier for a noteId",
            "type": "string"
          },
        "title": {
          "description": "The unique identifier for a title",
          "type": "string"
        },
        "content": {
          "description": "The unique identifier for a content",
          "type": "string"
        },
        "date": {
          "description": "The unique identifier for a date",
          "type": "string"
        }
      },
      "required": [ "noteId" ]
  }